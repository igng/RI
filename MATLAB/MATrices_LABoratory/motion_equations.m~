function eq = motion_equations(Q, q, M, ~, t, i)

    d = Q(1:3, 4);
    R = Q(1:3, 1:3);
    qp = diff(q,t);
    dp = diff(d, t);
    Jt = subs(dp, qp, ones(1,6));
    JJt = simplify(transpose(Jt)*Jt);
    Bt = M(1:i)*JJt;
    Tt = (qp(1:i)*Bt*transpose(qp(1:i)))/2;
    eq = R;
    Rp = diff(R, t);
